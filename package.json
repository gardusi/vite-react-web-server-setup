{
  "name": "vite-react-web-server-setup",
  "version": "0.1.0",
  "description": "Vite React web server setup",
  "main": "index.ts",
  "author": "Giovanni Gardusi",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start:dev": "node --experimental-specifier-resolution=node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
    "start": "vite build && NODE_ENV=production node --experimental-specifier-resolution=node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts"
  },
  "dependencies": {
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "mappersmith": "^2.43.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "vite-express": "^0.14.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
